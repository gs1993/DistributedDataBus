{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "RabbitMqSettings": {
    "Host": "localhost",
    "VirtualHost": "/",
    "UserName": "guest",
    "Password": "guest"
  },
  "GrpcSettings": {
    "Connections": [
      {
        "Name": "OrderService",
        "Url": "https://localhost:5001"
      },
      {
        "Name": "BookService",
        "Url": "https://localhost:5002"
      }
    ]
  },
  "RedisSettings": {
    "Url": "localhost",
    "Password": "testPass"
  },
  "IpRateLimitOptions": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "RealIPHeader": "X-Real-IP",
    "ClientIdHeader": "X-ClientId",
    "HttpStatusCode": 429,
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "60s",
        "Limit": 10
      }
    ]
  }
}
